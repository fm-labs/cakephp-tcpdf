{
    "name": "fm-labs/cakephp-tcpdf",
    "description": "Tcpdf plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=8.2",
        "cakephp/cakephp": "^5.0",
        "tecnickcom/tcpdf": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Tcpdf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tcpdf\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
